{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "@mate-academy/eslint-config",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "react"],
  "rules": {},
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
